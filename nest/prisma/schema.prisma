generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model user{
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  upDatedAt DateTime @updatedAt

  name String?
  email String
  hash String
}

model bookmark{
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  upDatedAt DateTime @updatedAt

  title String
  description String
  link String?
}